<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.user.dao.LikeDao">
    <insert id="insert" parameterType="com.user.pojo.Like">
      insert into t_likes (userid,like_route,like_carNum,atime)
      values (#{userid,jdbcType=CHAR},#{like_route,jdbcType=VARCHAR},#{like_carNum,jdbcType=VARCHAR},NOW())
    </insert>
</mapper>